<%= simple_form_for(@variant) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :AAchange %>
    <%= f.input :genotype %>
    <%= f.input :drugrelation %>
    <%= f.input :information %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
