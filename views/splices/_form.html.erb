<%= simple_form_for(@splice) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :sample_id %>
    <%= f.input :ensembl %>
    <%= f.input :event %>
    <%= f.input :coordinates %>
    <%= f.input :dpsi %>
    <%= f.input :pval %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
